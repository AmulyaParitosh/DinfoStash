{
  "$defs": {
    "Achievement": {
      "properties": {
        "type": {
          "description": "Type of achievement",
          "maxLength": 50,
          "minLength": 1,
          "title": "Achievement Type",
          "type": "string"
        },
        "title": {
          "description": "Title of the achievement",
          "maxLength": 100,
          "minLength": 1,
          "title": "Title",
          "type": "string"
        },
        "data": {
          "description": "Detailed information about the achievement",
          "minLength": 1,
          "title": "Data",
          "type": "string"
        },
        "link": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Link to the Achievement for validation",
          "title": "Achievement Link"
        }
      },
      "required": [
        "type",
        "title",
        "data"
      ],
      "title": "Achievement",
      "type": "object"
    },
    "Address": {
      "properties": {
        "line1": {
          "description": "First line of the address",
          "maxLength": 100,
          "minLength": 1,
          "title": "Address Line 1",
          "type": "string"
        },
        "line2": {
          "anyOf": [
            {
              "maxLength": 100,
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": "",
          "description": "Second line of the address, if any",
          "title": "Address Line 2"
        },
        "city": {
          "description": "City of the address",
          "maxLength": 50,
          "minLength": 1,
          "title": "City",
          "type": "string"
        },
        "state": {
          "description": "State of the address",
          "maxLength": 50,
          "minLength": 1,
          "title": "State",
          "type": "string"
        },
        "country": {
          "description": "Country of the address",
          "maxLength": 50,
          "minLength": 1,
          "title": "Country",
          "type": "string"
        },
        "pin_code": {
          "description": "Postal code of the address",
          "maximum": 999999,
          "minimum": 100000,
          "title": "Postal Code",
          "type": "integer"
        }
      },
      "required": [
        "line1",
        "city",
        "state",
        "country",
        "pin_code"
      ],
      "title": "Address",
      "type": "object"
    },
    "ContactInfo": {
      "properties": {
        "type": {
          "description": "Type of contact information (e.g., email, phone)",
          "maxLength": 50,
          "minLength": 3,
          "title": "Contact Type",
          "type": "string"
        },
        "display": {
          "description": "Display name for the contact information",
          "maxLength": 100,
          "minLength": 3,
          "title": "Display Name",
          "type": "string"
        },
        "link": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "format": "email",
              "type": "string"
            },
            {
              "format": "phone",
              "maxLength": 64,
              "minLength": 7,
              "type": "string"
            }
          ],
          "description": "Link to the contact information",
          "title": "Contact Link"
        },
        "fa_icon": {
          "description": "Font Awesome icon name",
          "maxLength": 50,
          "minLength": 3,
          "title": "Font Awesome Icon",
          "type": "string"
        }
      },
      "required": [
        "type",
        "display",
        "link",
        "fa_icon"
      ],
      "title": "ContactInfo",
      "type": "object"
    },
    "Education": {
      "properties": {
        "institute": {
          "description": "Name of the educational institute",
          "maxLength": 100,
          "minLength": 1,
          "title": "Institute",
          "type": "string"
        },
        "degree": {
          "description": "Degree obtained or being pursued",
          "maxLength": 100,
          "minLength": 1,
          "title": "Degree",
          "type": "string"
        },
        "status": {
          "description": "Current status of the education (e.g., completed, ongoing)",
          "maxLength": 20,
          "title": "Status",
          "type": "string"
        },
        "year": {
          "description": "Year of graduation or expected graduation",
          "maximum": 2100,
          "minimum": 1900,
          "title": "Year",
          "type": "integer"
        },
        "marks": {
          "allOf": [
            {
              "$ref": "#/$defs/Marks"
            }
          ],
          "description": "Marks obtained in the education"
        }
      },
      "required": [
        "institute",
        "degree",
        "status",
        "year",
        "marks"
      ],
      "title": "Education",
      "type": "object"
    },
    "Experience": {
      "properties": {
        "organization": {
          "description": "Name of the organization",
          "maxLength": 100,
          "minLength": 1,
          "title": "Organization",
          "type": "string"
        },
        "position": {
          "description": "Position held at the organization",
          "maxLength": 100,
          "minLength": 1,
          "title": "Position",
          "type": "string"
        },
        "date": {
          "description": "Date or duration of the experience",
          "maxLength": 50,
          "minLength": 1,
          "title": "Date",
          "type": "string"
        },
        "link": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Link to the organization or project",
          "title": "Link"
        },
        "location": {
          "anyOf": [
            {
              "maxLength": 100,
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Location of the experience",
          "title": "Location"
        },
        "descriptions": {
          "description": "List of descriptions of the role and responsibilities",
          "items": {
            "type": "string"
          },
          "title": "Descriptions",
          "type": "array"
        },
        "technologies": {
          "description": "List of technologies used",
          "items": {
            "type": "string"
          },
          "title": "Technologies",
          "type": "array"
        }
      },
      "required": [
        "organization",
        "position",
        "date",
        "descriptions",
        "technologies"
      ],
      "title": "Experience",
      "type": "object"
    },
    "Marks": {
      "properties": {
        "type": {
          "description": "Type of marks (e.g., percentage, GPA)",
          "maxLength": 20,
          "minLength": 1,
          "title": "Marks Type",
          "type": "string"
        },
        "value": {
          "description": "Value of the marks",
          "exclusiveMinimum": 0.0,
          "maximum": 100.0,
          "title": "Marks Value",
          "type": "number"
        }
      },
      "required": [
        "type",
        "value"
      ],
      "title": "Marks",
      "type": "object"
    },
    "PersonalInfo": {
      "properties": {
        "name": {
          "description": "Name of the person",
          "maxLength": 100,
          "minLength": 1,
          "title": "Name",
          "type": "string"
        },
        "photo": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "description": "Absolute file path to the photo",
          "title": "Photo"
        },
        "address": {
          "allOf": [
            {
              "$ref": "#/$defs/Address"
            }
          ],
          "description": "Address information"
        },
        "contact_infos": {
          "description": "List of contact information",
          "items": {
            "$ref": "#/$defs/ContactInfo"
          },
          "title": "Contact Information",
          "type": "array"
        },
        "short_infos": {
          "description": "List of short information snippets",
          "items": {
            "type": "string"
          },
          "title": "Short Information",
          "type": "array"
        }
      },
      "required": [
        "name",
        "photo",
        "address",
        "contact_infos",
        "short_infos"
      ],
      "title": "PersonalInfo",
      "type": "object"
    },
    "Project": {
      "properties": {
        "name": {
          "description": "Name of the project",
          "maxLength": 100,
          "minLength": 1,
          "title": "Project Name",
          "type": "string"
        },
        "link": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Link to the project",
          "title": "Project Link"
        },
        "type": {
          "anyOf": [
            {
              "maxLength": 50,
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Type of project",
          "title": "Project Type"
        },
        "organization": {
          "anyOf": [
            {
              "maxLength": 100,
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Organization under which the project was done",
          "title": "Organization"
        },
        "time": {
          "anyOf": [
            {
              "maxLength": 50,
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Time period of the project",
          "title": "Time Period"
        },
        "technologies": {
          "description": "List of technologies used in the project",
          "items": {
            "type": "string"
          },
          "title": "Technologies",
          "type": "array"
        },
        "description": {
          "description": "Description of the project",
          "minLength": 1,
          "title": "Description",
          "type": "string"
        }
      },
      "required": [
        "name",
        "technologies",
        "description"
      ],
      "title": "Project",
      "type": "object"
    },
    "Skill": {
      "properties": {
        "type": {
          "description": "Type of skill (e.g., technical, soft)",
          "maxLength": 50,
          "minLength": 1,
          "title": "Skill Type",
          "type": "string"
        },
        "values": {
          "description": "List of specific skills",
          "items": {
            "type": "string"
          },
          "title": "Skill Values",
          "type": "array"
        }
      },
      "required": [
        "type",
        "values"
      ],
      "title": "Skill",
      "type": "object"
    }
  },
  "properties": {
    "personal_info": {
      "allOf": [
        {
          "$ref": "#/$defs/PersonalInfo"
        }
      ],
      "description": "Personal information of the individual",
      "title": "Personal Information"
    },
    "educations": {
      "description": "List of educational qualifications",
      "items": {
        "$ref": "#/$defs/Education"
      },
      "title": "Educations",
      "type": "array"
    },
    "skills": {
      "description": "List of skills",
      "items": {
        "$ref": "#/$defs/Skill"
      },
      "title": "Skills",
      "type": "array"
    },
    "experience": {
      "description": "List of work experiences",
      "items": {
        "$ref": "#/$defs/Experience"
      },
      "title": "Experience",
      "type": "array"
    },
    "projects": {
      "description": "List of projects",
      "items": {
        "$ref": "#/$defs/Project"
      },
      "title": "Projects",
      "type": "array"
    },
    "achievements": {
      "description": "List of achievements",
      "items": {
        "$ref": "#/$defs/Achievement"
      },
      "title": "Achievements",
      "type": "array"
    }
  },
  "required": [
    "personal_info",
    "educations",
    "skills",
    "experience",
    "projects",
    "achievements"
  ],
  "title": "ResumeData",
  "type": "object"
}